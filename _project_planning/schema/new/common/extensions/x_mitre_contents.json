{
    "$id": "https://raw.githubusercontent.com/mitre-attack/attack-data-model/main/schema/common/extensions/x_mitre_contents.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "MITRE Contents",
    "description": "Specifies the objects contained within the collection.",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "object_ref": {
                "type": "string",
                "description": "The id of the referenced object."
            },
            "object_modified": {
                "$ref": "http://raw.githubusercontent.com/oasis-open/cti-stix2-json-schemas/stix2.1/schemas/common/timestamp.json",
                "description": "The modified time of the referenced object. It MUST be an exact match for the modified time of the STIX object being referenced."
            }
        },
        "required": [
            "object_ref",
            "object_modified"
        ],
        "additionalProperties": false
    },
    "minItems": 1
}